{
    "env": {
        "browser": true,
        "node": false
    },
    "ignorePatterns": [
        "database/index.js",
        "admin/category-reports.js",
        "bootstrap/*.js"
    ],
    "rules": {
        "no-undef": "off",
        "no-unused-vars": "off",
        "prefer-const": "off",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off"
    }
}
